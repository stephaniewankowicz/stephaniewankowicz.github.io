---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Stephanie Wankowicz's Blog</title>
  <link href="{{ site.url }}"/>
  <link type="application/atom+xml" rel="self" href="{{site.url}}/blog.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{site.url}}</id>
  <author>
    <name>Stephanie Wankowicz</name>
    <!-- <email>you@example.com</email> -->
  </author>

  {% for post in site.posts limit:20 %}
  <entry>
    <id>{{site.url}}{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="{{site.url}}{{ blog.url }}"/>
    <title>{{ blog.title }}</title>
    <published>{{ blog.date | date_to_xmlschema }}</published>
    <updated>{{ blog.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{blog.author}}</name>
      <uri>{{blog.url}}</uri>
    </author>
    <content type="html">{{ blog.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
